		"ejs": "^3.1.10",
		"express": "^4.21.2",
		"express-ejs-layouts": "^2.5.1",
		"express-session": "^1.18.1",
		"glob": "^11.0.1",
		"morgan": "^1.10.0",
		"multer": "1.4.5-lts.1",
		"pg": "^8.13.1",
		"qrcode": "^1.5.4",
