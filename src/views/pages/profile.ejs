                  <div class="submit clearfix">
                    <button type="submit">
                      <i class="fi flaticon-send"></i>
                    </button>
                  </div>
                </form>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- end container -->
    </div>
    <div class="wpo-lower-footer">
      <div class="container">
        <div class="row">
          <div class="col col-xs-12">
            <p class="copyright">
              Copyright &copy; 2022 Solun by <a href="index.html">wpOceans</a>.
              All Rights Reserved.
            </p>
          </div>
        </div>
      </div>
    </div>
    <div class="shape-2">
      <img src="/assets/images/footer-shape-1.png" alt="" />
    </div>
  </footer>
  <!-- end of wpo-site-footer-section -->
</div>
