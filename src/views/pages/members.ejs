            <a href="/profile">
              <div class="grid">
                <div class="grid-img">
                  <img src="assets/images/team/1.jpg" alt="" />
                  <div class="social-single-link">
                    <a href="#"><i class="ti-linkedin"></i></a>
                  </div>
                </div>
                <div class="text">
                  <h3><a href="team-single.html">Burton Messer</a></h3>
                  <span>Senior Engineer</span>
                </div>
              </div>
            </a>
          </div>
          <% }) %>
