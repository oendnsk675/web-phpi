<!-- Include header -->
<%- include('../components/header.ejs') %>

<!-- Dynamic content injected here -->
<body class="wpo-dashboard">
  <aside class="wpo-sidebar">
    <div class="wpo-sidebar-widget">
      <div class="wpo-widget-title">
        <img src="/assets/images/Logo-HPI.webp" width="50" alt="Logo HPI" />
        <h6>Nama Instansi</h6>
      </div>
    </div>

    <div class="wpo-sidebar-widget">
      <div class="wpo-widget-menu">
        <ul>
          <li>
            <a href="/admin/dashboard">
              <i class="fa fa-dashboard"></i>
              <span>Dashboard</span>
            </a>
          </li>
          <li>
            <a href="/admin/members">
              <i class="fa fa-user"></i>
              <span>User</span>
            </a>
          </li>
        </ul>
      </div>
    </div>

    <div class="wpo-sidebar-widget-logout">
      <div class="wpo-widget-menu">
        <ul>
          <li>
            <a href="/admin/dashboard">
              <i class="fa fa-sign-out" aria-hidden="true"></i>
              <span>Logout</span>
            </a>
          </li>
        </ul>
      </div>
    </div>
  </aside>
  <div class="sidebar-helper"></div>
  <main class="px-5 py-5"><%- body %></main>

  <script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
</body>
<!-- Konten halaman dinamis -->

<%- include('../components/ui/sweetalert.ejs') %>

<!-- Include footer -->
<%- include('../components/footer.ejs') %>
